{
  "name": "cation",
  "version": "1.0.0",
  "description": "Electron's issue triage bot",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project '.'",
    "start": "probot run ./lib/index.js",
    "postinstall": "tsc",
    "prettier:check": "prettier --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codebytere/cation.git"
  },
  "author": "Shelley Vohr (@codebytere)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codebytere/cation/issues"
  },
  "homepage": "https://github.com/codebytere/cation#readme",
  "devDependencies": {
    "@types/bunyan": "^1.8.5",
    "@types/express": "^4.16.1",
    "@types/node": "^11.9.4",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "smee-client": "^1.0.2",
    "tslint": "^5.12.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@octokit/rest": "^16.16.0",
    "github-issue-parser": "0.0.2",
    "probot": "~7.0.0",
    "semver": "^5.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
