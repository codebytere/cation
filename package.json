{
  "name": "cation",
  "version": "1.0.0",
  "description": "Electron's issue triage bot",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "lint": "tslint --project '.'",
    "start": "probot run ./lib/index.js",
    "postinstall": "tsc",
    "prettier:check": "prettier --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codebytere/cation.git"
  },
  "author": "Shelley Vohr (@codebytere)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codebytere/cation/issues"
  },
  "homepage": "https://github.com/codebytere/cation#readme",
  "devDependencies": {
    "@types/bunyan": "^1.8.6",
    "@types/express": "^4.17.9",
    "@types/node": "^11.15.39",
    "@octokit/webhooks-definitions": "^3.19.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.19.1",
    "smee-client": "^1.2.2",
    "tslint": "^5.20.1",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@octokit/rest": "^18.0.10",
    "probot": "^10.17.2",
    "semver": "^5.7.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  }
}
